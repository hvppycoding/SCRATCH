import { ref, onMounted, watch } from 'vue';
import FileExplorer from './FileExplorer.vue';
import AttachmentsPanel from './AttachmentsPanel.vue';
import MonacoEditor from './MonacoEditor.vue';

const props = defineProps<{
  node: any;
  initialAttachments?: any[];
}>();

const emit = defineEmits(['update:attachments']);

const attachments = ref<any[]>(props.initialAttachments || []);
const currentContent = ref('');
const currentLanguage = ref('python');
const currentFilePath = ref('');

const addAttachment = (item: any) => {
  // Check if already exists
  const exists = attachments.value.some(a => a.path === item.path && a.range === item.range);
  if (!exists) {
    attachments.value.push(item);
    emit('update:attachments', attachments.value);
  }
};

const removeAttachment = (index: number) => {
  attachments.value.splice(index, 1);
  emit('update:attachments', attachments.value);
};

const openFile = async (path: string) => {
  try {
    const response = await fetch('/hyperflow/fs/read', {
      method: 'POST',
      body: JSON.stringify({ path }),
    });
    const data = await response.json();
    if (data.error) {
      console.error(data.error);
      return;
    }
    currentContent.value = data.content;
    currentFilePath.value = path;
    
    // Detect language
    if (path.endsWith('.py')) currentLanguage.value = 'python';
    else if (path.endsWith('.js')) currentLanguage.value = 'javascript';
    else if (path.endsWith('.ts')) currentLanguage.value = 'typescript';
    else if (path.endsWith('.json')) currentLanguage.value = 'json';
    else if (path.endsWith('.md')) currentLanguage.value = 'markdown';
    else if (path.endsWith('.html')) currentLanguage.value = 'html';
    else if (path.endsWith('.css')) currentLanguage.value = 'css';
    else currentLanguage.value = 'plaintext';
    
  } catch (e) {
    console.error(\"Failed to open file\", e);
  }
};

const addSelectionAsAttachment = (selection: any) => {
  if (!currentFilePath.value) return;
  
  const item = {
    path: currentFilePath.value,
    range: selection.range // e.g. \"L10-L20\"
  };
  addAttachment(item);
};

watch(attachments, (newVal) => {
  // Update the node widget value if needed, handled by parent wrapper usually
  // But here we emit event
}, { deep: true });

</script>

